.feedlist
	
	&_header
		font-size 16px
		color $blue
		a
			text-decoration underline
	
	&_item
		padding 25px 35px
		display flex
		justify-content space-between
		align-items center
			
	&_excerpt
		font-size 16px
		margin 10px 0
		word-wrap break-word
		
	&_img
		display flex
		justify-content center
		align-items center
		width 65px
		height 65px
		img
			max-height 65px
			max-width 65px